@model FactsGreet.Web.ViewModels.Shared.CompactPaginationViewModel
<div class="container">
    <div class="row">
        <div class="col"></div>
        <div class="col-auto m-1">
            <a href="@($"{Model.Url}&page={Model.CurrentPage - 1}")"
               class="btn btn-secondary">
                Previous
            </a>
        </div>
        <div class="col-auto m-1">
            <a href="@($"{Model.Url}&page={Model.CurrentPage + 1}")"
               class="btn btn-secondary">
                Next
            </a>
        </div>
        <div class="col"></div>
    </div>
</div>
